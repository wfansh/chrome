{
	"manifest_version": 3,
	"name": "XHS - Downloader",
	"description": "This extension will download a video from XHS",
	"version": "1.0",
	"action": {
		"default_icon": "logo.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"activeTab",
		"scripting",
		"tabs",
		"downloads"
	]
}